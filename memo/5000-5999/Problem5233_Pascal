var a: Array[1..3,1..3] of Integer; i,j,cavab: LongInt;
begin
  for i:=1 to 3 do
    begin
      for j:=1 to 3 do read(a[i,j]);
      readln();
    end;
  cavab:=a[1,1]*(a[2,2]*a[3,3]-a[2,3]*a[3,2])-a[1,2]*(a[2,1]*a[3,3]-a[2,3]*a[3,1])+a[1,3]*(a[2,1]*a[3,2]-a[2,2]*a[3,1]);  
  writeln(cavab);  
end.
