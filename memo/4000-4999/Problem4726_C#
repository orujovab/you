using System;

namespace Problem4716
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            string setir = Console.ReadLine();
            int ilk = setir.IndexOf('f');
            int son = setir.LastIndexOf('f');
            if (ilk > -1)
            {
                Console.WriteLine(ilk == son ? ilk + "" : ilk + " " + son);
            }
        }
    }
}
