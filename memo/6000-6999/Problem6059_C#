using System;

namespace Problem6059
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            int t = int.Parse(Console.ReadLine());
            for (int i = 0; i < t; i++)
            {
                int n = int.Parse(Console.ReadLine());
                Console.WriteLine(Math.Pow(n/2 + 1, 2));
            }
        }
    }
}
