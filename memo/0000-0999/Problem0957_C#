using System;

namespace Problem957
{
    internal class Program
    {
        public static int ReqemCemi(int x)
        {
            int cem = 0;
            while (x > 0)
            {
                cem = cem + x%10;
                x = x/10;
            }
            return cem;
        }

        public static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            double kok = Math.Sqrt(ReqemCemi(n));
            Console.WriteLine("{0:N3}", kok);
        }
    }
}
