using System;

namespace Problem697
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            string cavab = "";
            for (int i = 0; i < n; i++)
            {
                double a = double.Parse(Console.ReadLine());
                Console.WriteLine("{0:N10}", a/6.0);
            }
        }
    }
}
