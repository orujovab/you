import java.util.Locale;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner in = new Scanner(System.in);
        in.useLocale(new Locale("US"));
        int n = in.nextInt();

        for(int i=0; i<n; i++) {
            System.out.format(Locale.US, "%.10f\n", in.nextDouble()/6.0);
        }
    }

}
