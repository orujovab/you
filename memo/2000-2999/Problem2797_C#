using System;
using System.Collections.Generic;

namespace Problem2797
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            string nomreler = Console.ReadLine();
            var massiv = nomreler.Split(' ');
            HashSet<int> kontakt = new HashSet<int>();
            for (int i = 0; i < n; i++)
            {
                kontakt.Add(int.Parse(massiv[i]));
            }
            Console.WriteLine(kontakt.Count);
        }
    }
}
