using System;

namespace Problem2165
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            string setir = Console.ReadLine();
            setir = setir.Trim();
            while (setir.Contains("  "))
            {
                setir = setir.Replace("  ", " ");
            }
            Console.WriteLine(setir);
        }
    }
}
