using System;

namespace Problem2041
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            if (n == 1) Console.WriteLine("Spring\nMarch\nApril\nMay");
            if (n == 2) Console.WriteLine("Summer\nJune\nJuly\nAugust");
            if (n == 3) Console.WriteLine("Autumn\nSeptember\nOctober\nNovember");
            if (n == 4) Console.WriteLine("Winter\nDecember\nJanuary\nFebruary");
        }
    }
}
